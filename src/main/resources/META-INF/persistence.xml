	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence.xsd"> 
	<persistence-unit name="persistence-unit" 
	transaction-type="RESOURCE_LOCAL"> 
