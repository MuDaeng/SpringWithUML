			<property name="javax.persistence.jdbc.url" value="${jdbc:mysql://localhost:3306/withu/serverTimeZone=UTC&amp;CharacterEncoding=UTF-8}" /> 
			<property name="javax.persistence.jdbc.user" value="${Mudaeng}" /> 
			<property name="javax.persistence.jdbc.password" value="${eotjd0588@}" /> 
		</properties> 
	</persistence-unit>
</persistence>